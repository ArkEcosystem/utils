{
    "name": "@arkecosystem/utils",
    "description": "Performance oriented implementations of commonly used functions in TypeScript.",
    "version": "1.1.7",
    "contributors": [
        "Brian Faust <brian@ark.io>",
        "Joshua Noack <joshua@ark.io>"
    ],
    "license": "MIT",
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepublishOnly": "yarn test && yarn build",
        "pretest": "yarn lint && yarn build",
        "compile": "./node_modules/typescript/bin/tsc",
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
        "clean": "del dist",
        "format": "yarn lint && yarn prettier",
        "lint": "./node_modules/eslint/bin/eslint.js src --ext .ts --fix",
        "prettier": "prettier --write \"./*.{ts,js,json,md}\" \"./src/**/*.{ts,js,json,md}\"",
        "bench": "yarn build && node benchmark/index.js",
        "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest"
    },
    "dependencies": {
        "@hapi/bourne": "^2.0.0",
        "deepmerge": "^4.2.2",
        "fast-copy": "^2.0.4",
        "fast-deep-equal": "^3.1.1",
        "fast-sort": "^2.0.0"
    },
    "devDependencies": {
        "@arkecosystem/commitlint": "^0.1.0",
        "@babel/core": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@commitlint/cli": "^8.2.0",
        "@faustbrian/benchmarker": "^0.1.2",
        "@hapi/boom": "^9.0.0",
        "@hapi/hapi": "^18.4.0",
        "@sindresorhus/tsconfig": "^0.7.0",
        "@types/benchmark": "^1.0.31",
        "@types/jest": "^24.0.23",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.12.14",
        "@types/prettier": "^1.19.0",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "benchmark": "^2.1.4",
        "big-number": "^2.0.0",
        "big.js": "^5.2.2",
        "bignumber.js": "^9.0.0",
        "codecov": "^3.6.1",
        "cross-env": "^6.0.3",
        "del-cli": "^3.0.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-jest": "^23.1.1",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-simple-import-sort": "^5.0.0",
        "full-icu": "^1.3.0",
        "husky": "^4.0.0",
        "hyperid": "^2.0.2",
        "jest": "^24.9.0",
        "jest-extended": "^0.11.2",
        "lint-staged": "^9.5.0",
        "lodash": "^4.17.15",
        "pluralize": "^8.0.0",
        "prettier": "^1.19.1",
        "random-object": "^1.3.4",
        "ts-jest": "^24.2.0",
        "type-fest": "^0.8.1",
        "typescript": "^3.7.3"
    },
    "engines": {
        "node": ">=10.x"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    }
}
